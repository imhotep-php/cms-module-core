{
  "name": "imhotep/cms-module-core",
  "type": "imhotep-cms-module",
  "autoload": {
    "psr-4": {
      "Imhotep\\Cms\\Modules\\Core\\": "src/"
    }
  },
  "require": {
    "composer/installers": "~2.0"
  },
  "extra": {
    "installer-paths": {
      "modules/{$vendor}/{$name}": ["type:imhotep-cms-module"],
      "plugins/{$vendor}/{$name}": ["type:imhotep-cms-plugin"],
      "themes/{$vendor}/{$name}": ["type:imhotep-cms-theme"]
    }
  },
  "minimum-stability": "dev"
}